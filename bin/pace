#! /usr/bin/env python
import sys
from time import sleep

pace = 10
if len(sys.argv) > 1:
    if sys.argv[1] == '-h':
        print('usage: pace [frequency]\n\n\tShoves [frequency] (defaults to 100) lines per second from stdin to stdout.')
        sys.exit(0)
    pace = float(sys.argv[1])

for line in sys.stdin:
    sys.stdout.write(line)
    sys.stdout.flush()
    sleep(1 / pace)
